{
  "expo": {
    "name": "WebViewApp",
    "slug": "WebViewApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shipglobal.weview",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
